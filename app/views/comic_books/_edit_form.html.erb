<%= simple_form_for @comic_book do |f| %>
  <%= f.input :series %>
  <%= f.input :name %>
  <%= f.input :issue %>
  <%= f.input :release_date %>
  <%= f.input :writer %>
  <%= f.input :artist %>
  <%= f.input :quantity %>
  <%= f.input :condition %>
  <%= f.input :description %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :pictures, as: :file, input_html: { multiple: true } %>
  <%= f.submit "Edit your Comic Book"%>
<% end %>
